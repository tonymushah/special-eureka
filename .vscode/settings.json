{
    "typescript.tsdk": "node_modules/typescript/lib",
    "material-icon-theme.files.associations": {
        "vite.storybook.config.ts": "storybook",
        "tauri.conf.json5": "tauri",
        "Tauri.toml": "tauri"
    },
    "[javascript]": {
        "editor.maxTokenizationLineLength": 2500,
        "editor.formatOnSave": true
    },
    "[javascriptreact]": {
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true
    },
    "[rust]": {
        "editor.formatOnSave": true
    },
    "[svelte]": {
        "editor.formatOnSave": true
    },
    "rust-analyzer.showUnlinkedFileNotification": false,
    "evenBetterToml.schema.associations": {
        "Tauri.toml" : "../node_modules/@tauri-apps/cli/schema.json"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "src-tauri/target": true,
        "src-tauri/data": true
    },
    "explorerExclude.backup": {}
}