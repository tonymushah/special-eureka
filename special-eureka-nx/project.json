{
  "name": "special-eureka-nx",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "special-eureka-nx/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/./special-eureka-nx",
        "main": "special-eureka-nx/src/index.ts",
        "tsConfig": "special-eureka-nx/tsconfig.lib.json",
        "assets": [
          "special-eureka-nx/*.md",
          {
            "input": "./special-eureka-nx/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./special-eureka-nx/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./special-eureka-nx",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./special-eureka-nx",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "special-eureka-nx/**/*.ts",
          "special-eureka-nx/generators.json",
          "special-eureka-nx/executors.json",
          "special-eureka-nx/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "special-eureka-nx/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
