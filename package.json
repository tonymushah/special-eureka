{
	"name": "special-eureka",
	"version": "0.1.7",
	"private": true,
	"scripts": {
		"vite:dev": "vite dev",
		"codegen": "graphql-codegen",
		"codegen:mangadex": "pnpm codegen -c ./src/lib/codegen/mangadex.ts",
		"vite:build_preview": "pnpm vite:build && pnpm vite:preview",
		"vite:build": "vite build --emptyOutDir",
		"vite:preview": "vite preview --port 9305",
		"dev": "tauri dev",
		"build": "tauri build",
		"preview": "pnpm vite:preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"clean-src-tauri": "cd src-tauri && cargo clean"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@parcel/watcher": "^2.3.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tauri-apps/cli": "^1.5.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"graphql": "^16.8.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0"
	},
	"type": "module",
	"dependencies": {
		"@graphiql/toolkit": "^0.9.1",
		"@svelteuidev/composables": "^0.15.4",
		"@svelteuidev/core": "^0.15.4",
		"@svelteuidev/dates": "^0.15.4",
		"@svelteuidev/motion": "^0.15.4",
		"@svelteuidev/preprocessors": "^0.15.4",
		"@svelteuidev/prism": "^0.15.4",
		"@tauri-apps/api": "^1.5.2",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@urql/svelte": "^4.0.4",
		"dayjs": "^1.11.10",
		"graphiql": "^3.0.10",
		"mizuki-urql-adapter": "^0.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"packageManager": "pnpm@8.11.0"
}
