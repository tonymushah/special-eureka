{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "mains",
	"description": "Capability for all main-* windows",
	"local": true,
	"windows": ["main", "main-*"],
	"permissions": [
		"core:default",
		"core:window:allow-start-dragging",
		"core:event:default",
        "clipboard-manager:default",
		"mangadex-desktop-api:allow-graphql",
		"mangadex-desktop-api:allow-subscriptions",
        "opener:default"
	]
}
